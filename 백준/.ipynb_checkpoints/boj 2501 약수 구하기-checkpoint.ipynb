   "source": [
    "n, k = map(int, input().split())\n",
    "\n",
    "cnt = 0\n",
    "result = 0\n",
    "for i in range(1, n+1):\n",
    "  if n % i == 0:\n",
    "    cnt += 1\n",
    "  if cnt == k:\n",
    "    result = i\n",
    "    break;\n",
    "\n",
    "if cnt < k:\n",
    "  print(0)\n",
    "else:\n",
    "  print(result)"
   ]
  }
