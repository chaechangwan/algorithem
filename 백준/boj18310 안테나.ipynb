   "metadata": {},
   "outputs": [],
   "source": [
    "n = int(input())\n",
    "house = list(map(int, input().split()))\n",
    "house.sort()\n",
    "answer = 0\n",
    "if n % 2 == 0:\n",
    "  answer = house[n//2 - 1]\n",
    "else:\n",
    "  answer = house[(n+1)//2 - 1]\n",
    "print(answer)"
   ]
  }
